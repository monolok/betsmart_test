<%# console%>
<h3>Surebet comming up: <%= @size%></h3>
<%= @show%>
<div class="bs-example" data-example-id="hoverable-table">
    <table class="table table-hover">
      <tbody>
      <% if @game_data.nil? %>
  	    <tr>
			<td>Pas de surebet en cours</td>
      <%= @game_data%>
        </tr>
      <%else%>
	      <% @game_data.each do |game|%>
	        <tr>
				<td><%= game%></td>
	        </tr>
	       <%end%>
       <%end%>
      </tbody>
    </table>
</div>
 
